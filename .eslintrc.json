{
    "env": {
        "node": true,
        "es2020": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "no-console": "off",
        "no-plusplus": [2, { "allowForLoopAfterthoughts": true}],
        "linebreak-style": "off",
        "import/extensions": "off"
    }
}
